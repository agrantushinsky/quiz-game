<Window x:Class="PIIIProject.ResultWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PIIIProject"
        mc:Ignorable="d"
        Title="ResultWindow" Height="800" Width="400" SizeToContent="Height" ResizeMode="NoResize">
    <Grid>
        <StackPanel Margin="8">
            <Label Content="Result" FontWeight="Bold"/>
            <TextBox x:Name="txbResult" IsReadOnly="True"/>

            <WrapPanel Margin="0,8,8,8">
                <Button Content="Save" Width="100" Click="btnSave_Click"/>
            </WrapPanel>
        </StackPanel>
    </Grid>
</Window>
